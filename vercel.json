{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.6.2" }
    },
    "outputDirectory": "public",
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/build/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "tms-test-beige.vercel.app",

        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "DB_CONNECTION": "pgsql",
        "DB_PORT": "5432",
        "SESSION_DRIVER": "database",
        "SESSION_LIFETIME": "120",
        "SESSION_ENCRYPT": "false",
        "SESSION_PATH": "/",
        "SESSION_DOMAIN": "null"

    }
}
